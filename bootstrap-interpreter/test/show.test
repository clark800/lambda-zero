main(input) := showInteger(+ 123)
+123
===============================================================================
main(input) := showInteger(- 123)
-123
===============================================================================
main(input) := showString(deduplicate("abc" ++ "bbd"))
"abcd"
===============================================================================
main(input) := showString(deduplicate("acbbcda"))
"acbd"
===============================================================================
main(input) := (0..10).map(positive)[3, 7].showList(showInteger)
[+3, +4, +5, +6]
===============================================================================
main(input) := showPair(" 12 3  4 ".split((= '3') <> head), showString, showString)
(" 12 ", "3  4 ")
===============================================================================
main(input) := showPair(" 12 3  4 ".splitOn("3"), showString, showString)
(" 12 ", "3  4 ")
===============================================================================
main(input) := showList(" 12 3  4 ".partitionOn(" "), showString)
["12", "3", "", "4", ""]
===============================================================================
main(input) := showList("12 3  4".partitionBy((= ' ')), showString)
["12", "3", "", "4"]
===============================================================================
main(input) := showList("a..b...c....d..".partitionOn(".."), showString)
["a", "b", ".c", "", "d", ""]
===============================================================================
main(input) := showList(words(" 12 3  4 "), `showList(showNatural))
[[49, 50], [51], [52]]
===============================================================================
main(input) := showString("   12 3  4 ".dropWhile((= ' ')))
"12 3  4 "
===============================================================================
main(input) := showList((5..9).map(positive), showInteger)
[+5, +6, +7, +8, +9]
===============================================================================
main(input) := showList(second(splitWhen((1 .. 5), (>= 3))), showNatural)
[3, 4, 5]
===============================================================================
main(input) := showList(sort([5, 88, 43, 22, 91, 17]), showNatural)
[5, 17, 22, 43, 88, 91]
